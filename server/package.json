{
  "name": "flowbites-server",
  "version": "1.0.0",
  "description": "Flowbites Marketplace Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "seed": "node src/scripts/seed.js",
    "seed:extras": "node src/scripts/seed-extras.js",
    "seed:all": "node src/scripts/seed.js && node src/scripts/seed-extras.js",
    "generate-placeholders": "node src/scripts/generate-placeholders.js",
    "health": "node ../scripts/health-check.js",
    "migrate-cloudinary": "node src/scripts/migrate-to-cloudinary.js",
    "migrate": "node ../scripts/migrations/run-migrations.js",
    "test": "MONGODB_URI=${MONGODB_URI:-mongodb://localhost:27017/flowbites-test} node tests/integration.test.js",
    "test:ci": "NODE_ENV=test MONGODB_URI=${MONGODB_URI:-mongodb://localhost:27017/flowbites-test} node tests/integration.test.js"
  },
  "keywords": [
    "marketplace",
    "templates",
    "mern"
  ],
  "author": "Flowbites Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "resend": "^6.9.2",
    "slugify": "^1.6.6",
    "stripe": "^14.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
